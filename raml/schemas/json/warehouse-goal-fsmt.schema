{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Warehouse Goal FSM transition",
  "description": "Finite state machine transition for agent communication",
  "type": "object",
  "properties": {
    "transition": {
      "type": "string",
      "enum": [
        "PROPOSED",
        "ADVISED",
        "REJECTED",
        "ACCEPTED",
        "CANCELED",
        "COMMITTED",
        "DECOMMITTED",
        "FAILED",
        "SATISFIED"
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "transition"
  ]
}
